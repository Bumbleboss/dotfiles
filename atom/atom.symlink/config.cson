'editor':
  'autoIndentOnPaste': true
  'hideGitIgnoredFiles': false
  'softWrapAtPreferredLineLength': true
  'fontFamily': 'Anonymous Pro'
  'softWrap': true
  'showIndentGuide': true
  'preferredLineLength': 120
  'showInvisibles': true
'core':
  'hideGitIgnoredFiles': false
  'disabledPackages': [
    'wrap-guide'
    'autocomplete-plus'
    'metrics'
    'exception-reporting'
    'welcome'
    'autoclose-html'
    'git-plus'
    'linter-csslint'
    'linter-jshint'
    'linter-coffeelint'
    'linter-rubocop'
    'linter-scss-lint'
    'linter'
    'vim-mode'
  ]
  'themes': [
    'neutron-ui'
    'neutron-syntax'
  ]
'spell-check':
  'grammars': [
    'text.plain'
    'source.gfm'
    'text.git-commit'
  ]
'Rdio': {}
'whitespace':
  'ignoreWhitespaceOnCurrentLine': false
'release-notes':
  'viewedVersion': '0.95.0'
'linter':
  'showStatusBarWhenCursorIsInErrorRange': true
'autoclose-html': {}
'emmet':
  'extensionsPath': '~/.dotfiles/emmet'
'merge-conflicts':
  'gitPath': '/opt/boxen/homebrew/bin/hub'
